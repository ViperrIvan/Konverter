try:
    valueSum = int(input())
    exchangeRate = float(input())
except:
    print('Ковбой на диком dick`е ')
def interpreter(value, exchgange):
    if value<=0 or exchgange<=0:
        print('отрицать надо не число а женщин')
    else:
        return value/exchgange
print(interpreter(valueSum, exchangeRate))
